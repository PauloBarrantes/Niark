public function Eratostenes(n)
	$criba[n] //Crea un vector de tama√±o n //
	// Vamos a desperdiciar el espacio 0 y 1 del vector//

	for($i = 2; i < n; ++i)
		criba[i] = False

	for($i = 2; i < sqrt(n); ++i)
		if(!criba[i])
			for($j = i; j < (n/i); j++)
				criba[i+j] = TRUE

	return criba // Retorna un vector//
}
